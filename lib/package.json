    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "wb-core-provider": "^0.4.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.0.1",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.5.1",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "eslint": "^8.47.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-wbox-frontend": "^0.3.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.6.2",
        "jest-environment-jsdom": "^29.6.2",
        "prettier": "^3.0.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typemoq": "^2.1.0",
        "typescript": "^5.1.6",
        "vite": "^4.4.9"
    },
    "scripts": {
        "cleanup:build": "rm -rf ./build/__examples__ && rm -rf ./build/__examples && rm -rf ./build/__tests__ && rm -rf ./build/Utils && rm ./build/index.d.ts && rm ./build/index.js",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:coverage": "jest --coverage --watchAll",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "dev": "vite",
        "build": "tsc --noEmit false && npm run cleanup:build",
        "preview": "vite preview",
        "pretty": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
