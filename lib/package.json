{
    "name": "wb-forms",
    "version": "0.4.1",
    "private": false,
    "main": "build/main.js",
    "types": "build/main.d.ts",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "wb-core-provider": "^0.4.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.14",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "cors": "^2.8.5",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint": "^8.37.0",
        "express": "^4.17.1",
        "prettier": "^2.8.7",
        "typemoq": "^2.1.0",
        "typescript": "^4.9.3",
        "vite": "^4.2.0"
    },
    "scripts": {
        "start:server": "node src/__server__/server.js",
        "cleanup:build": "rm -rf ./build/__examples__ && rm -rf ./build/__examples && rm -rf ./build/__tests__ && rm -rf ./build/Utils && rm ./build/index.d.ts && rm ./build/index.js",
        "test": "react-scripts test",
        "test:coverage": "react-scripts test --coverage --watchAll",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "dev": "npm run start:server & vite",
        "dev-no-server": "vite",
        "build": "tsc --noEmit false && npm run cleanup:build",
        "preview": "vite preview"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
